}, {
"base": "Node",
"class": "GameManager",
"language": "GDScript",
"path": "res://game/managers/game_manager_class.gd"
}, {
"base": "Control",
"class": "GameOver",
"language": "GDScript",
"path": "res://game/gui/game_over_class.gd"
}, {
"base": "TileMap",
"class": "GameTilemap",
"language": "GDScript",
"path": "res://game/tilemaps/arena_tilemap.gd"
}, {
"base": "KinematicBody2D",
"class": "Player",
"language": "GDScript",
"path": "res://game/pixel/player_class.gd"
}, {
"base": "KinematicBody2D",
"class": "Stray",
